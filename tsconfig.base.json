{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["@org/source"],
    "baseUrl": ".",
    "paths": {
      "@org/prisma": ["generated/prisma"],
      "@org/prisma/*": ["generated/prisma/*"],
      "@org/prisma-module": ["libs/prismaModule/src"],
      "@org/prisma-module/*": ["libs/prismaModule/src/*"],
      "@org/nextFetch": ["libs/nextFetch/src/index.ts"],
      "@org/nextFetch/*": ["libs/nextFetch/src/*"]
    }
  }
}
